{
  "name": "git-stats-visualizer",
  "displayName": "Git Stats Visualizer",
  "description": "Beautiful visualization of team code contributions with interactive charts and insights",
  "version": "0.1.0",
  "publisher": "local-test",
  "author": {
    "name": "local-test"
  },
  "license": "MIT",
  "icon": "resources/icon.png",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Visualization",
    "Other"
  ],
  "keywords": [
    "git",
    "statistics",
    "visualization",
    "team",
    "contribution"
  ],
  "galleryBanner": {
    "color": "#6C5CE7",
    "theme": "dark"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/YOUR_USERNAME/git-stats-visualizer"
  },
  "bugs": {
    "url": "https://github.com/YOUR_USERNAME/git-stats-visualizer/issues"
  },
  "homepage": "https://github.com/YOUR_USERNAME/git-stats-visualizer#readme",
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "git-stats-visualizer.showStats",
        "title": "Show Git Stats Visualization"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node --no-deprecation ./out/test/runTest.js"
  },
  "dependencies": {
    "simple-git": "^3.20.0",
    "moment": "^2.29.4"
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@types/vscode": "^1.60.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.26.0",
    "typescript": "^5.2.2"
  }
}
